<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <OutputType>Library</OutputType>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Version>15.2.2</Version>
    <AssemblyVersion>2.0.0.0</AssemblyVersion>
    <FileVersion>2.0.0.0</FileVersion>
    <Company>Kalix, Inc</Company>
    <Product>Kalix</Product>
    <Copyright>Copyright 2021</Copyright>
    <Description>Core library that abstracts your storage layer, adds encryption/compression</Description>
    <PackageTags>Azure Blob Table Queue Encryption Compression</PackageTags>
    <Authors>Felix Jorkowski</Authors>
    <RepositoryUrl>https://github.com/KalixHealth/Kalix.Leo</RepositoryUrl>
    <PackageReleaseNotes>Optimistic store returns lock with access to underlying task</PackageReleaseNotes>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="System.IO.Pipelines" Version="5.0.1" />
    <PackageReference Include="System.Linq.Async" Version="5.0.0" />
    <PackageReference Include="System.Threading.Channels" Version="5.0.0" />
    <PackageReference Include="System.Threading.Tasks.Dataflow" Version="5.0.0" />
  </ItemGroup>
</Project>